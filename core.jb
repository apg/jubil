;;;; Startup library for jubil.

;;; booleans
(def false? (= false))
(def true? (= true))

(def zero? (= 0))
(def pos? (> 0.0))
(def neg? (< 0.0))

(puts/3 "zero? 1: " (zero? 1) "\n")
(puts/3 "zero? 0: " (zero? 0) "\n")
(puts/3 "zero? -1: " (zero? -1) "\n")

(puts/3 "pos? 1: " (pos? 1) "\n")
(puts/3 "pos? 0: " (pos? 0) "\n")
(puts/3 "pos? -1: " (pos? -1) "\n")

(puts/3 "neg? 1: " (neg? 1) "\n")
(puts/3 "neg? 0: " (neg? 0) "\n")
(puts/3 "neg? -1: " (neg? -1) "\n")
